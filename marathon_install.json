{
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "hpreston/myhero_tropo",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 5000,
                "hostPort": 0
            }]
        },
        "forcePullImage": true
    },
    "healthChecks": [{
        "protocol": "TCP",
        "portIndex": 0
    }],
    "id": "/myhero/tropo",
    "instances": 1,
    "cpus": 0.5,
    "mem": 32,
    "env": {
        "myhero_app_server": "http://myhero-app",
        "myhero_app_key": "SecureApp",
        "myhero_tropo_secret": "SecureTropo",
        "myhero_tropo_user":"tropouser",
        "myhero_tropo_pass":"tropopass",
        "myhero_tropo_prefix":"tropoprefix",
        "myhero_tropo_url":"http://myhero-tropo"
    }
}